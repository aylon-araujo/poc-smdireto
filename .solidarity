{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "Node": [{
      "rule": "cli",
      "binary": "node",
      "semver": ">=16.0"
    }],
    "Package JSON": [{
      "rule": "file",
      "location": "./package.json"
    }],
    ".ENV": [{
      "rule": "file",
      "location": "./.env",
      "error": "Please create a .env file in the root of your project or request the file to another developer."
    }],
    "NPM": [{
      "rule": "cli",
      "binary": "npm",
      "semver": ">=8.0"
    }],
    "Yarn": [{
      "rule": "cli",
      "binary": "yarn",
      "version": "--version",
      "semver": ">=1.22.0"
    }]
  }
}
